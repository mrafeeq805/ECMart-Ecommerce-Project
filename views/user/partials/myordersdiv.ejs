<div>
</div>

<!-- orders -->
<div class="flex flex-col gap-3 p-3">

    <% list.forEach((item)=>{ %>


    <!-- card -->
    <a href="/home/orderdetails/<%= item.order_id %>">
        <div class="flex w-full items-center justify-between border-2 p-2 rounded-md">
            <div class="flex w-max">
                <div class="bg-gray-100 rounded-md w-4/12 px-4 py-2 flex justify-center items-center">
                    <img class=" object-cover" src="<%= item.products[0].image %>" alt="h">
                </div>
                <div class="ml-2 w-full">
                    <span class="block"><%= item.products[0].title.slice(0,45) %></span>
                    <span class="text-sm text-gray-500"><%= item.products[0].category %>  | </span>
                    <span class="text-sm text-gray-500">Qty.<%= item.products[0].quantity %></span>
                    <span class="text-xs block text-gray-500 leading-6">#<%= item.order_id %></span>
                </div>
            </div>
            
           
        </div>
    </a>
    
    <% }) %>

</div>
</div>