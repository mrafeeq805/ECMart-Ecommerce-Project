<div class="grid grid-cols-5 md:grid-cols-10 gap-3 px-3">
    <% categorylist.forEach((item)=>{ %>
        <!-- category card -->
        <a href="/categoryproducts/<%= item.name %>">
            <div class="flex flex-col items-center">

                <div class="h-[60px] w-[60px] md:h-[90px] md:w-[90px] rounded-full bg-[#1a91ff1f] p-2 ">
                    <div class="md:h-20 md:w-20 h-14 w-14">
                        <img class="" src="<%= item.icon %>" alt="ivon">
                    </div>
    
                </div>
                <span class="text=[#00000080] text-sm font-medium md:text-lg"><%= item.name %></span>
            </div>
        </a>
        
    <% }) %>
    
</div>